  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime-corejs3": "^7.12.1",
    "@neuekit/neuekit": "^1.0.0",
    "autoprefixer": "^9.7.1",
    "browser-sync": "^2.26.13",
    "core-js": "^3.3.6",
    "custom-event-polyfill": "1.0.7",
    "element-closest": "^3.0.0",
    "fancybox": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "postcss-cli": "6.1.3",
    "prettier": "^2.1.2",
    "promise-polyfill": "8.1.3",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "sass": "^1.23.3",
    "smoothscroll-polyfill": "^0.4.2",
    "stickyfilljs": "^2.1.0",
    "stylelint": "^13.7.2",
    "stylelint-order": "^4.1.0",
    "svg4everybody": "^2.1.9",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "serve": "browser-sync start --https --proxy 'https://brothers-we-stand-shop.myshopify.com?preview_theme_id=109190447260' --no-notify --files '.' --ignore 'node_modules'",
    "scss": "sass --no-source-map -s compressed ./styles:./assets",
    "autoprefixer": "postcss --use autoprefixer --replace ./assets/style.css --no-map",
    "build:css": "run-s scss autoprefixer",
    "build:js": "rollup --config",
    "build": "run-s build:css build:js",
    "watch:css": "onchange './styles/**/*.scss' -- npm run build:css",
    "watch:js": "onchange './scripts/**/*.js' -- npm run build:js",
    "watch:theme": "theme open --allow-live && theme watch --allow-live",
    "watch": "run-p watch:theme watch:css watch:js",
    "start": "npm run watch",
    "prettier": "prettier --check '**/*.{js,scss,css,html,json,md,yml,yaml}'",
    "prettier:fix": "prettier --write '**/*.{js,scss,css,html,json,md,yml,yaml}'",
    "style": "stylelint '**/*.{css,scss}'",
    "style:fix": "stylelint --fix '**/*.{css,scss}'"
  },
  "dependencies": {
    "@shopify/theme-product": "^4.0.2",
    "@shopify/theme-product-form": "^4.0.2",
    "@shopify/theme-sections": "^4.0.2",
    "jquery": "^3.5.1",
    "tiny-slider": "^2.9.3"
  }
}
