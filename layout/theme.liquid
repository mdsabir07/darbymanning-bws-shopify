<!doctype html>
<html>
<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  <link rel="stylesheet" href="https://use.typekit.net/eah1lkg.css">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/fancyapps/fancybox@3.5.7/dist/jquery.fancybox.min.css" />
  <link rel="stylesheet" href="http://www.starplugins.com/sites/starplugins/js/cloudzoom/cloudzoom.css"> 
  {{ content_for_header }} <!-- Header hook for plugins -->

  {{ 'style.css' | asset_url | stylesheet_tag }}
  {{ '//ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js' | script_tag }}
  <script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer="defer"></script>
  <script src="{{ 'lazysizes.js' | asset_url }}" async="async"></script>
  <script src="{{ 'cloudzoom.js' | asset_url }}" async="async"></script>
  <script src="https://cdn.jsdelivr.net/gh/fancyapps/fancybox@3.5.7/dist/jquery.fancybox.min.js"></script>

  {{ 'polyfills.js' | asset_url | script_tag }}
  {{ 'application.js' | asset_url | script_tag }}
</head>
<body class="template-{{ request.page_type | handle }}">

  {% section 'header' %}
  <main class="main" role="main">
    {{ content_for_layout }}
  </main>
  {% section 'pre-footer' %}
  {% section 'footer' %}
</body>
</html>
