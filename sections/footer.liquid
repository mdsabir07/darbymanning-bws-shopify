
<footer class="footer" data-section-id="{{ section.id }}" data-section-type="footer-section" style="background-color:{{section.settings.color_footer_bg}};">
    <div class="o-grid  u-flex u-pV-32">
            {%- for block in section.blocks -%}
                {% case block.settings.width %}
                    {% when '16%' %}                    
                    {%- assign block_width = 'u-size-2@m' -%}
                    {%- assign block_width_mobile = 'u-size-6@m' -%}
                    {%- assign menu_columns = '' -%}
                    {% when '25%' %}                    
                    {%- assign block_width = 'u-size-3@m' -%}
                    {%- assign block_width_mobile = 'u-size-6@m' -%}
                    {% when '33%' %}                    
                    {%- assign block_width = 'u-size-4@m' -%}
                    {%- assign block_width_mobile = 'u-size-6@m' -%}
                    {%- assign menu_columns = 'footer-nav__ul--col2' -%}
                    {% when '50%' %}                    
                    {%- assign block_width = 'u-size-6@m' -%}
                    {%- assign block_width_mobile = 'u-size-6@m' -%}
                    {% when '66%' %}                    
                    {%- assign block_width = 'u-size-8@m' -%}
                    {% when '75%' %}                    
                    {%- assign block_width = 'u-size-9@m' -%}
                    {% when '100%' %}
                    {%- assign block_width = 'u-size-12@m' -%}
                {% endcase %}

            <div class="o-grid--item{% if section.blocks.size == 1 %} u-textCenter{% endif %} {{ block_width }}"
                        {{ block.shopify_attributes }}>
                <div class="block  block__info  block__info--p-tb0  border  border--darkblue">
                        {%- if block.settings.title -%}
                            <p class="footer-nav__title">{{ block.settings.title | escape }}</p>
                        {%- endif -%}
                {%- case block.type -%}                   
                    {%- when 'link_list' -%}
                     <div>
                        {%- assign footer_linklist = block.settings.menu -%}
                        <ul class="footer-nav__ul {{menu_columns}}">
                            {%- for link in linklists[footer_linklist].links -%}
                            <li class="footer-nav__li">
                                <a href="{{ link.url }}"
                                {% if link.current %} aria-current="page"{% endif %} class="footer-nav__a">
                                {{ link.title | escape }}
                                </a>
                            </li>
                            {%- endfor -%}
                        </ul>
                      </div>
                      {%- when 'search' -%}
                      <div class="search-bar u-mV-20" role="dialog" aria-modal="true" aria-label="{{ 'general.search.placeholder' | t }}" data-predictive-search-drawer>
                        <div class="search-bar__interior">
                          <div class="search-form__container" data-search-form-container>
                            <form class="search-form search-bar__form" action="{{ routes.search_url }}" method="get" role="search">
                              <div class="search-form__input-wrapper">
                                <input
                                  type="text"
                                  name="q"
                                  placeholder="{{ 'general.search.placeholder' | t }}"
                                  role="combobox"
                                  aria-autocomplete="list"
                                  aria-owns="predictive-search-results"
                                  aria-expanded="false"
                                  aria-label="{{ 'general.search.placeholder' | t }}"
                                  aria-haspopup="listbox"
                                  class="search-form__input search-bar__input join__input"                                  
                                />
                                <input type="hidden" name="options[prefix]" value="last" aria-hidden="true" />                                
                              </div>
                      
                              <button class="search-bar__submit search-form__submit"
                                type="submit"
                                data-search-form-submit>
                                {% include 'icon-search' %}      
                                <span class="icon__fallback-text">{{ 'general.search.submit' | t }}</span>                          
                              </button>
                            </form>
                                                  
                          </div>
                        </div>
                      </div>
                      {%- when 'spacer' -%}
                    <div class="block-spacer" style="background-color:{{block.settings.bg_color}};">

                      </div>                      
                      
                {%- endcase -%}
             </div> 
            </div>
        {%- endfor -%}
    </div>     
</footer>

{% if request.page_type == 'index' %}
  {% assign potential_action_target = shop.url | append: routes.search_url | append: "?q={search_term_string}" %}
  <script type="application/ld+json">
    {
      "@context": "http://schema.org",
      "@type": "WebSite",
      "name": {{ shop.name | json }},
      "potentialAction": {
        "@type": "SearchAction",
        "target": {{ potential_action_target | json }},
        "query-input": "required name=search_term_string"
      },
      "url": {{ shop.url | append: page.url | json }}
    }
  </script>
{% endif %}


